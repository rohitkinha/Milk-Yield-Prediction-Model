<%-include("header") -%>
    <div>
        <h1 class="text-center">
            Milk Prediction Model
        </h1>
        <h3 class="text-center">
            Here are the Predicted Values
        </h3>
        <div>
            <div class="row">
                <div class="col-md-4"></div>
                <div class="col-md-4">
                    <div class="form-container" style="background-color:rgb(164, 188, 146);">
                            <div class="row">
                                <% for(var i=0;i<milkYeild.length;i++){ %>

                                    <div class="text-white form-label h5">
                                        <p>Predicted Value for cattle <%=i+1%> - <%=milkYeild[i]%> Litres/day
                                        </p>
                                    </div>
                                    <% } %>
                            </div>
                    </div>
                </div>
                <div class="col-md-4"></div>
            </div>
        </div>
    </div>


    <%-include("footer") -%>